      <script>
        let data = <%- JSON.stringify(projects)%>;
        console.log(data)
        function  openfile(id) {
          $('#theImg1').remove();$('#theImg2').remove();$('#theImg3').remove();$('#theImg4').remove();
          console.log("got something")
          console.log(id);
          let selected = data.filter((data)=>data._id == id);
          console.log(selected)
          if(selected.length>0){
            $('#exampleModal').modal("show")
            $('#project-header').text(selected[0].title);
            $('#project-content').text(selected[0].content);
            if(selected[0].image1){
                let some = selected[0].image1.toString('base64');
            }
             $('#one').prepend($('<img>',{id:'theImg1',src:`data:image/png;base64,${some}`}))
            }
            if(selected[0].image2){
                let some2 = selected[0].image2.toString('base64');
              $('#two').prepend($('<img>',{id:'theImg2',src:`data:image/png;base64,${some2}`}))
            }
            if(selected[0].image3){
                let some3 = selected[0].image3.toString('base64');
              $('#three').prepend($('<img>',{id:'theImg3',src:`data:image/png;base64,${some3}`}))
            }
            if(selected[0].image4){
                let some4 = selected[0].image4.toString('base64');
             $('#four').prepend($('<img>',{id:'theImg4',src:`data:image/png;base64,${some4}`))
            }
          }

        }

      </script>